<p style="color: green"><%= notice %></p>

<h1>Books</h1>

<div id="books">
  <% @books.each do |book| %>
    <!-- <%= render book %>  -->
    <p>
	    <strong>Title:</strong>
	    <%= book.title %>
    </p>
    <p>
	    <strong>Image url:</strong>
	    <%= book.image_url %>
    </p>
    
    <p>
      <%= link_to "Show this book", book %>
    </p>
  <% end %>
</div>

<%= link_to "New book", new_book_path %>
